<h1>Display your Pirate here</h1>


<div class="pirate">
  <h3>Name: <%= @pirate.name %></h3><br>
  <h4>Wight: <%= @pirate.weight %></h4>
  <h5>Height: <%= @pirate.height %></h4>
</div><br>

<h1>Ships</h1>
<% @ships.each do |ship| %>
  <div class="ship">
    <ul>
    <li>Name: <%= ship.name %></li>
    <li>Type: <%= ship.type %></li>
    <li>Booty: <%= ship.booty %></li>
  </ul>
  </div><br>
<% end %>


